	<body>
		<div class="navbar navbar-inverse navbar-fixed-top">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#main-navbar-collapsible">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<a class="navbar-brand" href="/" data-localize="navbar.console">Hexabus Console</a>
			</div>
			<div class="collapse navbar-collapse" id="main-navbar-collapsible">
				<ul class="nav navbar-nav">
					<li class="<%= active_nav == 'dashboard' ? 'active' : '' %>">
						<a href="/" data-localize="navbar.main">Main</a>
					</li>
					<li class="<%= active_nav == 'statemachines' ? 'active' : '' %>">
						<a href="/statemachine" data-localize="navbar.statemachines">Statemachines</a>
					</li>
				</ul>
				<ul class="nav navbar-nav pull-right">
					<li data-ng-controller="healthController">
						<a class="btn-danger btn" data-ng-show="errorState" href="/wizard/current" role="button">
							<span class="glyphicon glyphicon-warning-sign"><span>
						</a>
					</li>
					<li class="dropdown <%= active_nav == 'configuration' ? 'active' : '' %>">
						<a class="dropdown-toggle" data-toggle="dropdown" data-localize="navbar.configuration">Configuration<b class="caret"></b></a>
						<ul class="dropdown-menu">
							<li><a href="/wizard/current" data-localize="navbar.network-status">Network status</a></li>
							<li><a href="/wizard/devices" data-localize="navbar.device-list">Devices</a></li>
							<li class="divider"></li>
							<li><a href="/wizard/reset" class="btn btn-danger" data-localize="navbar.reset-device">Reset device</a></li>
						</ul>
					</li>
					<li class="<%= active_nav == 'about' ? 'active' : '' %>">
						<a class="pull-right" href="/about" data-localize="navbar.about">About</a>
					</li>
				</ul>
